{
	"_version": "1.21.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "com.kyyte.icecream",
		"type": "application",
		"i18n": {
			"bundleUrl": "i18n/i18n.properties",
			"supportedLocales": [
				""
			],
			"fallbackLocale": ""
		},
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{title}}",
		"description": "{{appDescription}}",
		"resources": "resources.json",
		"ach": "ach",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		},
		"dataSources": {
			"mainService": {
				"uri": "/service/icecream/",
				"type": "OData",
				"settings": {
					"annotations": [],
					"localUri": "localService/metadata.xml",
					"odataVersion": "4.0"
				}
			}
		}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_hcw",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": {
			"viewName": "com.kyyte.icecream.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.115.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.suite.ui.commons": {},
				"sap.suite.ui.microchart": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.kyyte.icecream.i18n.i18n",
					"supportedLocales": [
						""
					],
					"fallbackLocale": ""
				}
			},
			"": {
				"dataSource": "mainService",
				"preload": true,
				"settings": {
					"synchronizationMode": "None",
					"operationMode": "Server",
					"autoExpandSelect": true,
					"earlyRequests": true
				}
			},
			"process": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/data/ProcessFlowData.json"
			},
			"business": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/data/BusinessData.json"
			},
			"suppliers": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/data/Suppliers.json"
			},
			"reviews": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/data/Reviews.json"
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "com.kyyte.icecream.view",
				"controlId": "tutorialApp",
				"controlAggregation": "pages",
				"transition": "slide",
				"bypassed": {
					"target": [
						"launchpad"
					]
				}
			},
			"routes": [
				{
					"pattern": "",
					"name": "home",
					"target": "startpage"
				},
				{
					"pattern": "processFlow",
					"name": "processFlow",
					"target": "processFlow"
				},
				{
					"pattern": "chartContainer",
					"name": "chartContainer",
					"target": "chartContainer"
				},
				{
					"pattern": "reviews",
					"name": "reviews",
					"target": "reviews"
				}
			],
			"targets": {
				"startpage": {
					"viewName": "Startpage",
					"viewLevel": 0
				},
				"processFlow": {
					"viewName": "ProcessFlow",
					"viewLevel": 1
				},
				"chartContainer": {
					"viewName": "ChartContainer",
					"viewLevel": 1
				},
				"reviews": {
					"viewName": "Reviews",
					"viewLevel": 1
				}
			}
		}
	}
}